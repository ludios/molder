#!/usr/bin/python

import sys

for line in sys.stdin:
	line = line.rstrip("\r\n")
	tab_i = line.find("\t")
	sp_i = line.find(" ")
	delim = "\t" if tab_i != -1 and (sp_i == -1 or tab_i < sp_i) else " "
	parts = line.split(delim)
	try:
		print format(int(parts[0]), ",d") + delim + delim.join(parts[1:])
	except ValueError:
		print format(float(parts[0]), ",.10f") + delim + delim.join(parts[1:])
