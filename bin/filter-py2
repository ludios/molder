#!/usr/bin/python

"""
Print out lines for which some Python 2.x lambda body returns a truthy value.
The current line can be accessed with the variable "line".

Examples:

filter-py2 "line.startswith('hi') and line.endswith('test')"
"""

# Import modules the user might want to use
import sys
import os
import string
import re
import json
import time
import operator

exec "line_filter = lambda line: " + sys.argv[1]

for line in sys.stdin:
	line = line.rstrip("\r\n")
	if line_filter(line):
		print line
