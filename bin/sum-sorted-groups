#!/usr/bin/python -B

import sys

sum_col = int(sys.argv[1]) # which column to sum
group_cols = map(int, sys.argv[2:]) # which column(s) to use as unique id

last_group = []
last_total = 0

for line in sys.stdin:
	cols = line.rstrip("\r\n").split("\t")
	group = []
	for c in group_cols:
		group.append(cols[c])

	if group != last_group:
		if last_group != []:
			print "\t".join(last_group + [str(last_total)])
		last_group = group
		last_total = 0

	try:
		last_total += int(cols[sum_col])
	except ValueError:
		last_total += float(cols[sum_col])

print "\t".join(last_group + [str(last_total)])
